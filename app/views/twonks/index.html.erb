<div id='most_recent_twonks'>
  <h1>Most Recently Twonked</h1>
  <%= render :partial => @most_recent_twonks %>
</div>

<div id='top_twonks'>
  <h1>Top Twonks</h1>
  <%= render :partial => @top_twonks %>
</div>

<div id='most_grudges'>
  <h1>Most Grudges</h1>
  <% for user in @users %>
    <%= user %> - <%= user.nominations.size %><br />
  <% end %>
